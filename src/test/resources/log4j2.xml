<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="stream-tributary">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="[%p] %d %c{2}.%M - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="com.monetate" level="DEBUG" additivity="false">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Root level="WARN">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>
</Configuration>